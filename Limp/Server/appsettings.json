{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthAutority": {
    "Address": "https://auth-api:1000/",
    "Endpoints": {
      "Register": "api/Auth/register",
      "Get-Token": "api/Auth/get-token",
      "GetUserName": "api/Users/GetUserName",
      "ExplicitRefreshTokens": "api/Auth/refresh-tokens-explicitly",
      "ValidateAccessToken": "api/Auth/validate-access-token",
      "RSAPublic": "api/Users/RSAPublic",
      "SubscribeToWebPush": "api/WebPush/notifications/subscribe",
      "RemoveWebPushSubscriptions": "api/WebPush/notifications/remove",
      "GetNotificationsByUserId": "api/WebPush/notifications",
      "CheckIfUserExist": "api/Users/user/{username}/exist",
      "GetTokenRefreshHistory": "api/Auth/refresh-history"
    }
  },
  "MessageBrokerSettings": {
    "bootstrap.servers": "79.137.202.134:9092",
    "group.id": "kafka-dotnet-getting-started",
    "auto.offset.reset": "earliest",
    "messageDispatcherHubAddress": "https://localhost:7273/messageDispatcherHub"
  },
  "Redis": {
    "Address": "etha-chat-redis:6937"
  }
}
