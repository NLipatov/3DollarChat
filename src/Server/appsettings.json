{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthAutority": {
    "Address": "http://etha-auth-api:8080/",
    "Endpoints": {
      "Register": "api/Auth/register",
      "Get-Token": "api/Auth/get-token",
      "GetUserName": "api/Users/GetUserName",
      "RefreshCredentials": "api/Auth/refresh",
      "ValidateCredentials": "api/Auth/validate",
      "RSAPublic": "api/Users/RSAPublic",
      "SubscribeToWebPush": "api/WebPush/notifications/subscribe",
      "RemoveWebPushSubscriptions": "api/WebPush/notifications/remove",
      "GetNotificationsByUserId": "api/WebPush/notifications",
      "CheckIfUserExist": "api/Users/user/{username}/exist",
      "GetTokenRefreshHistory": "api/Auth/refresh-history",
      "UsernameByCredentials": "api/Auth/username"
    }
  },
  "HlsApi": {
    "Address": "http://etha-hls-api:9001"
  },
  "DriveAPI": {
    "Address": "http://go-drive:10001"
  }
}
